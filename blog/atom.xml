
---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 	<title>About the Williams and Brandon Lab</title>
	<link href="https://williamsandbrandonlab.github.io/about/atom.xml" rel="self"/>
	<link href="https://williamsandbrandonlab.github.io/about"/>
 	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://williamsandbrandonlab.github.io/about</id>
	<author>
		<name>Williams and Brandon Lab</name>
		<email>templetorilynn@gmail.com</email>
	</author>


	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://williamsandbrandonlab.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://williamsandbrandonlab.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
